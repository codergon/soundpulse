{
  "expo": {
    "name": "SoundPulse",
    "slug": "SoundPulse",
    "scheme": "soundpulse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "safe",
          "trust",
          "uniswap",
          "rainbow",
          "metamask"
        ]
      },
      "bundleIdentifier": "com.alphatron.SoundPulse"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "23553f2a-5749-4693-b642-b08ead77de71"
      }
    }
  }
}
